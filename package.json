{
  "name": "transport-library",
  "version": "0.0.0-semantically-released",
  "description": "Transport layer for your app",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "browser": {
    "./dist/index.cjs.js": "./dist/index.cjs.min.js",
    "./dist/index.esm.js": "./dist/index.esm.min.js"
  },
  "types": "dist/types/index.node.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "build:js": "rollup -c --failAfterWarnings",
    "build": "rimraf dist && npm run lint && npm run build:types && npm run build:js",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gzaripov/transport-library.git"
  },
  "keywords": [
    "http",
    "client",
    "transport",
    "request"
  ],
  "author": "gzaripov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gzaripov/transport-library/issues"
  },
  "homepage": "https://github.com/gzaripov/transport-library#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "17.0.0",
    "@rollup/plugin-node-resolve": "11.0.1",
    "@rollup/plugin-typescript": "4.1.2",
    "@types/jest": "25.2.2",
    "@types/node": "13.11.1",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "7.2.1",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-prettier": "3.1.2",
    "http-status-codes": "1.4.0",
    "jest": "26.0.1",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "rollup": "2.35.1",
    "rollup-plugin-terser": "6.1.0",
    "semantic-release": "17.0.4",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3"
  }
